{
  "name": "feedmake",
  "version": "0.3.0",
  "description": "Generate a subscribable feed from a static website",
  "main": "src/index.ts",
  "bin": {
    "feedmake": "node build/src/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "start": "npm run build && node build/src/index.js",
    "lint": "eslint src/**.ts",
    "fix": "npm run lint -- --fix && prettier -w src/**.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meisekimiu/feedmake.git"
  },
  "keywords": [
    "rss",
    "xml",
    "html"
  ],
  "author": "Natalie Martin",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/meisekimiu/feedmake/issues"
  },
  "homepage": "https://github.com/meisekimiu/feedmake#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@tsconfig/node20": "^20.1.6",
    "@types/argparse": "^2.0.10",
    "@types/luxon": "^2.0.9",
    "@types/node": "^17.0.16",
    "@types/xml2js": "^0.4.9",
    "argparse": "^2.0.1",
    "gitlog": "^5.1.0",
    "luxon": "^2.5.2",
    "micromark": "^4.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^5.9.2",
    "xml2js": "^0.5.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier -w"
    ]
  }
}
